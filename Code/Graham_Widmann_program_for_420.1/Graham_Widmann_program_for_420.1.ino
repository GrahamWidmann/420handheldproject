#define TFT_DC     9

Adafruit_ST7735 tft = Adafruit_ST7735(TFT_CS,  TFT_DC, TFT_RST);

#define TFT_SCLK 15   // set these to be whatever pins you like!
#define TFT_MOSI 16   // set these to be whatever pins you like!

