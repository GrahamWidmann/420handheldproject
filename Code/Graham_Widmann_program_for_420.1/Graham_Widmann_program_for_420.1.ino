#define black 0x0000
#define white 0xFFFFFF
#define red 0xFF0000
#define green 0x00FF00
#define blue 0x0000FF
#define pink 0xFFC0CB

#define PIN 6

Adafruit_NeoPixel strip = Adafruit_NeoPixel(15, PIN, NEO_GRB + NEO_KHZ800);

#define KEY_LEFT_SHIFT  0x81

//ACTUAL CODE BELOW


int bS = digitalRead(3);
boolean letitgo = false;
boolean bitmapDrawn = false;

RotaryEncoder myEnc (5, 4);

int rot = 0;
int mode = 0;// for the potentiometer
int lastMode = -1;
int lastMenu = -1;
int numMode = 9;
long oldPosition  = -999;
int dir = 0;
int lastColor = -1;
int i = 0;

void setup(void) {

  tft.initR(INITR_144GREENTAB);

  pinMode(2, INPUT_PULLUP);//automatically turns to high //move
  pinMode(3, INPUT_PULLUP);//automatically turns to high //hand
  pinMode(7, INPUT_PULLUP);//automatically turns to high //pen
  pinMode(A0, INPUT_PULLUP);//automatically turns to high //zoom
  pinMode(A1, INPUT_PULLUP);//automatically turns to high //brush
  Keyboard.begin();

  tft.fillScreen(black);

  tft.setTextColor (red, white);

  strip.begin();
  strip.show();

  Serial.begin(9600);

  tft.setRotation(1);
}

void loop() {
  
  tft.setTextSize(1);

  if (lastMode != mode) {
    tft.fillScreen(black);
    lastMode = mode;
    lastMenu = -1;
    bitmapDrawn = false;
  }

  switch (mode) {
    case 0: Splash(); break;
    case 1: buttonPress(); Hand(); break;
    case 2: buttonPress(); Brush(); break;
    case 3: menu(); break;
    case 4: buttonPress(); Pen(); break;
    case 5: buttonPress(); Zoom(); break;
    case 6: buttonPress(); Move(); break;
    case 7: help(); break;
    case 8: about(); break;
    case 9: colors(); break;
  }

  changeBrush();

}





